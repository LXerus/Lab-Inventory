<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Configuration.ProductTypeConfigurationController">
    <children>
        <TextField fx:id="config_tdp_txtfl_id" layoutX="125.0" layoutY="50.0" />
        <TextField fx:id="config_tdp_txtfl_reg" layoutX="125.0" layoutY="100.0" />
        <TableView fx:id="config_tdp_table_tdp" layoutX="49.0" layoutY="178.0" prefHeight="200.0" prefWidth="414.0">
            <columns>
                <TableColumn fx:id="config_tdp_clmn_idtdp" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="config_tdp_clmn_nombretdp" prefWidth="214.0" text="Tipo de product" />
                <TableColumn fx:id="config_tdp_clmn_desctdp" prefWidth="124.0" text="Descripcion" />
            </columns>
        </TableView>
        <Button fx:id="config_tdp_btn_buscar" layoutX="365.0" layoutY="50.0" mnemonicParsing="false" onAction="#getProductType" prefWidth="75.0" text="Buscar" />
        <Button fx:id="config_tdp_btn_nuevo" layoutX="365.0" layoutY="88.0" mnemonicParsing="false" onAction="#createProductType" prefHeight="50.0" prefWidth="165.0" text="Nueva clase de Producto" />
        <Button fx:id="config_tdp_btn_cancelar" layoutX="456.0" layoutY="50.0" mnemonicParsing="false" onAction="#cancel" prefWidth="75.0" text="Cancelar" />
        <Label layoutX="50.0" layoutY="50.0" text="ID:" />
        <Label layoutX="50.0" layoutY="100.0" text="Registro:" />
    </children>
</AnchorPane>
